{
  "name": "lrc-file-parser",
  "version": "2.3.2",
  "description": "Javascript-based lrc file parser",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
        "types": "./index.d.ts",
        "import": "./index.js",
        "default": "./index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch --dts",
    "build": "tsup --dts",
    "lint": "tsc",
    "web": "rollup -c && rollup -c rollup.config.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/lrc-file-parser.git"
  },
  "keywords": [
    "javascript", "lrc", "web"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/lrc-file-parser/issues"
  },
  "homepage": "https://github.com/lyswhut/lrc-file-parser#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "rollup": "^3.20.7",
    "terser": "^5.17.1",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
